{
    "manifest_version": 3,
    "version":"0.3.1",
    "name": "threadsLiker",
    "description": "A Chrome extension to FullFill your Threads MSG IT Wing Sewa needs",
    "action":{
        "default_popup": "index.html"
    },
    "permissions":[
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://www.threads.net/*"
    ],
    "icons": {
        "128": "threadsLiker_logo_128.png"
    },
    "background":{
        "service_worker": "service-worker.js",
        "type": "module"
    }
}