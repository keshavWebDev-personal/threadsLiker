{
    "manifest_version": 3,
    "version":"0.0.1",
    "name": "threadsLiker",
    "description": "A Chrome extension to FullFill your Threads MSG IT Wing Sewa needs",
    "action":{
        "default_popup": "index.html"
    },
    "permissions":[
        "scripting"
    ],
    "host_permissions": [
